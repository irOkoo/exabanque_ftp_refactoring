<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="ir_cron_base_connector" model="ir.cron">
        <field name="name">Exabanque sftp</field>
        <field name="model_id" ref="model_base_connector"/>
        <field name="state">code</field>
        <field name="code">model._run_cron()</field>
        <field name="interval_number">5</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="doall">False</field>
        <field name="active">False</field>
    </record>

    <record id="ir_cron_transaction_log" model="ir.cron">
        <field name="name">Exabanque Transactions</field>
        <field name="model_id" ref="model_log_transaction"/>
        <field name="state">code</field>
        <field name="code">model._run_cron()</field>
        <field name="interval_number">5</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="doall">False</field>
        <field name="active">False</field>
    </record>
</odoo>
